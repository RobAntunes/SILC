{
  "name": "silc-protocol",
  "version": "0.1.0",
  "description": "Self-Interpreting Local Communication (SILC) Protocol - AI-native communication protocol using mathematical signal patterns",
  "keywords": [
    "ai",
    "communication",
    "protocol",
    "signal-processing",
    "shared-memory",
    "ai-native",
    "mathematical-signals"
  ],
  "homepage": "https://github.com/yourusername/silc-protocol",
  "bugs": {
    "url": "https://github.com/yourusername/silc-protocol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/silc-protocol.git"
  },
  "license": "MIT",
  "author": "Your Name <your.email@example.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./signal": {
      "import": "./dist/signal/index.mjs",
      "require": "./dist/signal/index.js",
      "types": "./dist/signal/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.mjs",
      "require": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./dialect": {
      "import": "./dist/dialect/index.mjs",
      "require": "./dist/dialect/index.js",
      "types": "./dist/dialect/index.d.ts"
    },
    "./transport": {
      "import": "./dist/transport/index.mjs",
      "require": "./dist/transport/index.js",
      "types": "./dist/transport/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "node tests/performance/runner.js",
    "benchmark": "tsx benchmarks/index.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "complexity": "npx complexity-report-html src/**/*.ts -o complexity-report.html",
    "audit:fix": "npm audit fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "typedoc",
    "release": "semantic-release"
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "lz4": "^0.6.5",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "semantic-release": "^23.1.1",
    "ts-jest": "^29.1.5",
    "tsx": "^4.16.2",
    "typedoc": "^0.26.4",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}